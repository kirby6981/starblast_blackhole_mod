var ships = [
  
  Barracuda_607,
  
  ];

this.options = {
  // see documentation for options reference
  root_mode: "survival",
  map_size: 200,
  ships: ships,
  survival_time: 1e10,
  asteroids_strength: 0.1,
  crystal_value: 200,
  custom_map: eye,
  radar_zoom: 0.000000001
};
// 10 minutes = 36000 ticks

const black_hole_time = 36000

let vx = 0;
let vy = 0;
let x = 0;
let y = 0;
let black_hole_size = 200;
let strength = black_hole_size / 200;

this.tick = function(game,ship) {
  // do mod stuff here ; see documentation
    for (let ship of game.ships) { 
      if (game.step < black_hole_time + 1 && game.step%60 === 0) {
        ship.setUIComponent({
              id:"timer",
                position:[25,10,50,40],
                clickable: false,
                visible: true,
                components: [
                  { type:"box",position:[25,5,50,30],fill:"#8F00FF80",stroke:"#8F00FF80",width:0},
                  { type:"text",position:[32.5,20,35,15],color:"#FFFFFF80",value: Math.ceil((black_hole_time/60)-(game.step / 60)) },
                  { type:"text",position:[25,0,50,30],color:"#FFFFFF80",value: "Time Until Black Hole" },
                    ]
                });
      }
      if (game.step > black_hole_time - 1) {
        if (game.step%100 === 0) {
          if (black_hole_size < 850) {
            black_hole_size += 5;
          }
          strength = black_hole_size / 200;
          ship.setUIComponent({
            id:"radar_background",
              position:[35,35,30,30],
              clickable: false,
              visible: true,
              components: [
                { type:"round",position:[(50 - (black_hole_size / 13.3333333333)) , (50 - (black_hole_size / 13.3333333333)),black_hole_size / 6.66666666667,black_hole_size / 6.66666666667],fill:"#FF000080",stroke:"#FF000080",width:2},
                  ]
              });
        }
        vx = ship.vx;
        vy = ship.vy;
        x = ship.x;
        y = ship.y;
        if (x < black_hole_size * 1.1 && x > -(black_hole_size * 1.1) && y < black_hole_size * 1.1 && y > -(black_hole_size * 1.1)) {
            if ((x > 1 || x < -1) && (y > 1 || y < -1) ) {
              ship.set({vx:vx + (-strength/x),vy:vy + (-strength/y)});
              if (x < 30 && y < 30 && x > -30 && y > -30) {
                ship.set({kill:true});
              }
            }
        }
      }
    }
};
